{  
  "manifest_version": 3,
  "name": "Block It",
  "version": "1.0",
  "description": "Welcome back to real world.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ],

  "icons": {
    "48": "icon (1).png", "128": "icon.png"
},

  "action":{
    "default_popup": "Popup.html",
    "default_title": "Block It"
},
  "host_permissions": ["https://*.youtube.com/*",
  "https://www.facebook.com/*",
  "https://www.netflix.com/*",
  "https://open.spotify.com/*",
  "https://discord.com/*"

]

}

